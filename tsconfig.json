{
    "extends": "@tsconfig/react-native/tsconfig.json",
    "compilerOptions": {
        "baseUrl": ".",
        "jsx": "react-jsx",
        "experimentalDecorators": true,
        "noImplicitAny": false,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "paths": {
            "@/*": [
                "src/*"
            ]
        }
    }
}